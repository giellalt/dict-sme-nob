      <tg xml:lang="nob">
        <t pos="Interj">oi</t>
        <t pos="Phrase">å dæven</t>
        <xg>
          <x>Vuoi ihán, don leat čeahppi.</x>
          <xt>Oi, så flink du er.</xt>
        </xg>
        <xg>
          <x>Vuoi ihán, dáppe lea moivvas.</x>
          <xt>Å dæven, det er rotete her.</xt>
        </xg>
      </tg>
    </mg>
  </e>
